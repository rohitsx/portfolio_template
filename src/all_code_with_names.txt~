./store/useAboutOptionStore.tsx
import { create } from 'zustand'

export type SetAboutProps = 'Intro' | 'Resume' | 'Career' | 'Education'

type Store = {
  aboutOption: SetAboutProps;
  setAboutOption: (v: SetAboutProps) => void;
}

export const useAboutOption = create<Store>()((set) => ({
  aboutOption: "Resume",
  setAboutOption: (v: SetAboutProps) => set(state => (
    { aboutOption: v }
  ))
}))

./store/useNavBarStore.tsx
import { create } from 'zustand'


type Store = {
  isOpen: boolean
  toggelIsOpen: () => void
}

export const useNavBarStore = create<Store>()((set) => ({
  isOpen: false,
  toggelIsOpen: () => set(state => ({isOpen: !state.isOpen}))
}))
